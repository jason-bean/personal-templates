<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name Default="GoogleChrome">GoogleChrome</Name>
  <Registry>https://registry.hub.docker.com/u/c0b0/chrome-stable/</Registry>
  <Repository>c0b0/chrome-stable</Repository>
  <Priviledged>false</Priviledged>
  <Network Default="bridge">bridge</Network>
  <Icon>https://upload.wikimedia.org/wikipedia/commons/8/87/Google_Chrome_icon_%282011%29.png</Icon>
  <Category>Network:Other</Category>
  <Config
    Type="Variable"
    Name="VNC Password"
    Target="VNC_PASSWORD"
    Default="hola;you"
    Description="VNC Password"
    Mask="false"
    Display="always"
    Required="false">hola;you</Config>
  <Config
    Type="Port"
    Name="Chrome Debugging Protocol Port"
    Target="9222"
    Default="9222"
    Description="Chrome Debugging Protocol Port"
    Display="always"
    Required="true">9222</Config>
    <Config
      Type="Port"
      Name="Chrome VNC Port"
      Target="5900"
      Default="5900"
      Description="Chrome VNC Port"
      Display="always"
      Required="true">5900</Config>
</Container>
