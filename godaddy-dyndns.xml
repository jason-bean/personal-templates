<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name Default="GoDaddy-DynDNS">GoDaddy-DynDNS</Name>
  <Description>
    GoDaddy DynDNS
  </Description>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/jasonbean/docker-godaddy-dyndns/</Registry>
  <GitHub>https://github.com/jason-bean/docker-godaddy-dyndns</GitHub>
  <Repository>jasonbean/docker-godaddy-dyndns</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Network Default="bridge">bridge</Network>
  <Config
    Type="Variable"
    Name="Check Frequency Minutes"
    Target="FREQ_MINUTES"
    Default="30"
    Description="Container Variable: FREQ_MINUTES"
    Display="always"
    Required="true">30</Config>
  <Config
    Type="Path"
    Name="Calibre Library Directory"
    Target="/config"
    Default="/mnt/user/appdata/godaddy-dyndns"
    Mode="rw"
    Description="This is where configuration and logs stored."
    Display="always"
    Required="true">/mnt/user/appdata/godaddy-dyndns</Config>
  <Icon>https://img1.wsimg.com/fos/help-v2/2.22.2-543/img/gd_logo.png</Icon>
  <Beta>False</Beta>
  <Category>Network:Management Network:Other</Category>
</Container>