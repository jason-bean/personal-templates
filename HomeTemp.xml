<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name Default="HomeTemp">HomeTemp</Name>
  <Registry>https://hub.docker.com/u/jasonbean/home_temp_monitor_webpack/</Registry>
  <Repository>jasonbean/home_temp_monitor_webpack</Repository>
  <Privileged>false</Privileged>
  <Icon>https://raw.githubusercontent.com/jason-bean/personal-templates/master/nodejs-512.png</Icon>
  <Category>Network:Other</Category>
  <Config
    Type="Port"
    Name="App Web Port"
    Target="8080"
    Default="8080"
    Mode="tcp"
    Description="App Web Port"
    Display="always"
    Required="true">8080</Config>
  <Config
    Type="Path"
    Name="Web App Config Directory"
    Target="/config"
    Default="/mnt/user/appdata/hometemp/"
    Mode="rw"
    Description="Where web app config data goes."
    Display="always"
    Required="true">/mnt/user/appdata/hometemp/</Config>
</Container>
